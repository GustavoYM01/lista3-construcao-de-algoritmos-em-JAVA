package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		double randomNumber = 0.0;
		
		for (int i = 1; i <= 20; i++) {
			System.out.print("Digite um número: ");
			randomNumber = sc.nextDouble();
			
			sc.nextLine();
		}

	}

}
